<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd" >

    <context:annotation-config/>



    <bean id="student1" class="yzhao.spring.io.StudentAnnotations.Student">
        <property name="name" value="zyh"/>
        <property name="age" value="20"/>
    </bean>

    <bean id="student" class="yzhao.spring.io.StudentAnnotations.Student">
        <property name="name" value="yzhao"/>
        <property name="age" value="23"/>
    </bean>

    <bean id="student2" class="yzhao.spring.io.StudentAnnotations.Student" primary="true">
        <property name="name" value="lj"/>
        <property name="age" value="18"/>
    </bean>

    <bean id="profile" class="yzhao.spring.io.StudentAnnotations.Profile">
    </bean>

</beans>